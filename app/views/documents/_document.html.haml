%tr
  %td= document.name
  %td= document.available
  %td= link_to t('.download'), document
  %td= document.user.username if document.user
  %td= document.updated_at
  %td= document.asset_file_size
  %td= link_to t('.edit'), edit_document_path(document)
  %td= link_to 'Destroy', document, :confirm => 'Are you sure?', :method => :delete